@model AdoNetExample.Models.EmployeeModel 
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>



<form action="~/Home/Edit" method="post">
    <input type="hidden" name="EmpId" value="@Model.EmpId" />
    <label>Employee Name</label>
    <input type="text" name="EmpName" class="form-control" value="@Model.EmpName" />
    <label>Salary</label>
    <input type="text" name="EmpSalary" class="form-control" value="@Model.EmpSalary" />
    <input type="submit" value="Update" class="btn btn-success" />
</form>